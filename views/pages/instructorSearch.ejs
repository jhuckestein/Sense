                <form method="get" action="/instructorSearch" class="instRetrieve1">
                    <input type="checkbox" id="exe1" name="exe1" required>
                    <label for="exe1">Execute this query</label><br>
                    <input type="submit" class="btn btn-primary instRetrieve1Button" value="Submit" />
