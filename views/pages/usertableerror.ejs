            <a href="/elements">Elements</a>
        </nav>
        <a href="#navPanel" class="navPanelToggle"><span class="fa fa-bars"></span></a>
    </div>
</header>

<!-- Main -->
<section id="main" class="wrapper">
    <!-- DB Logic starts... -->

    <div class="container">
        <h2>User Table Database Results</h2>

        <ul>
            <% results.forEach(function(r) { %>
            <li><%= r.usernumber %>  - <%= r.userfirstname %> - <%= r.userlastname %> - <%= r.useremail %> - <%= r.useraddress %> - <%= r.userstreet %> - <%= r.usercity %> - <%= r.userstate %> - <%= r.userzip %></li>
            <% }); %>
        </ul>
    </div>
    <hr class="major" />
    <ul class="actions">
        <li><a href="/administratives" class="button special">Return to Administrative Page</a></li>
    </ul>
    <hr />
</section>

